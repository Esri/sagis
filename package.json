{
  "name": "SaGIS",
  "version": "0.1.0",
  "homepage": "https://github.com/eile/sagis",
  "private": true,
  "description": "Scalable GIS Renderer for the 3D ArcGIS Maps SDK for JavaScript",
  "keywords": [
    "ArcGIS",
    "javascript",
    "map",
    "3D",
    "visualization",
    "analytics",
    "spatial",
    "data-driven",
    "gis",
    "Esri",
    "displaywall"
  ],
  "license": "SEE LICENSE IN license.txt",
  "bugs": "https://github.com/eile/sagis/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:eile/sagis.git"
  },
  "browser": {
    "buffer": false,
    "crypto": false,
    "fs": false,
    "path": false,
    "process": false
  },
  "engines": {
    "node": ">=16.15"
  },
  "dependencies": {
    "@arcgis/core": "^4.27.0",
    "@types/ws": "~8.5.4",
    "ts-node": "^10.9.1",
    "ws": "~8.13.0"
  },
  "devDependencies": {
    "@types/grunt": "~0.4.28",
    "@types/node": "~16.11.68",
    "@typescript-eslint/eslint-plugin": "~6.7.3",
    "@typescript-eslint/parser": "~6.7.3",
    "eslint": "~8.50.0",
    "eslint-config-prettier": "~9.0.0",
    "eslint-plugin-import": "~2.28.1",
    "eslint-plugin-jsdoc": "~46.8.2",
    "eslint-plugin-prettier": "~5.0.0",
    "eslint-plugin-react": "~7.33.2",
    "eslint-plugin-regexp": "~1.15.0",
    "eslint-plugin-unicorn": "~48.0.1",
    "grunt": "~1.6.1",
    "npm-run-all": "~4.1.5",
    "prettier": "~3.0.3",
    "source-map-support": "~0.5.21",
    "tslib": "~2.6.2",
    "tsx": "~3.13.0",
    "typescript": "~5.2.2"
  },
  "scripts": {
    "build": "cross-env-shell NODE_OPTIONS=\"--max-old-space-size=8192\" tsc -p tsconfig-ci.json",
    "watch": "npm-run-all codegen --parallel watch:ts:dev watch:ts:noEmit",
    "watch:ts:dev": "tsx ./scripts/transpile.ts --watch",
    "watch:ts:noEmit": "cross-env-shell NODE_OPTIONS=\"--max-old-space-size=8192\" tsc -p tsconfig-ci.json --watch --noEmit",
    "sagisServer": "ts-node --project scripts/tsconfig.json ./scripts/sagis-server.ts"
  }
}
